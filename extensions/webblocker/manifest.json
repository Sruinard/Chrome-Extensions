{
  "manifest_version": 2,

  "name": "Neuralics",
  "description": "Improves your focus and productivity.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "permissions": [
          "storage",
          "notifications",
          "contextMenus",
          "tabs",
          "background",
          "webRequest",
          "webRequestBlocking",
          "<all_urls>"
        ]
}