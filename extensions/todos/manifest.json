{
  "manifest_version": 2,

  "name": "todo",
  "description": "Improves your focus and productivity.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },

  "permissions": [
          "storage",
          "notifications",
          "contextMenus",
          "tabs",
          "background",
          "webRequest",
          "webRequestBlocking",
          "<all_urls>"
        ]
}